@page
@model TakeAwayExample2.Pages.Admin.MenuItemBatch.BatchUpsertModel
@{
}
<h1 class="text-primary">Upload the Excel file</h1>
<br />
<form method="post" asp-action="BatchUpsert" enctype="multipart/form-data">
    <div class="container">
        <div class="form-group row">
            <input type="file" name="excelFile" class="form-control" id="excel-upload" style="border: none;" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Upload</button>
        </div>
    </div>
</form>
